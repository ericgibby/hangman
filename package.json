{
	"name": "hangman",
	"version": "0.1.0",
	"private": true,
	"homepage": "https://ericgibby.github.io/hangman",
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"format": "prettier --ignore-path \"node_modules/**\" --write \"**/*.{ts,js,json,scss,css,html,md}\"",
		"predeploy": "yarn build",
		"deploy": "gh-pages -d build"
	},
	"dependencies": {
		"classnames": "^2.2.6",
		"foundation-sites": "^6.5.3",
		"prop-types": "^15.7.2",
		"react": "^16.8.6",
		"react-dom": "^16.8.6"
	},
	"devDependencies": {
		"gh-pages": "^2.0.1",
		"husky": "^2.2.0",
		"node-sass": "^4.13.1",
		"prettier": "^1.17.0",
		"pretty-quick": "^1.10.0",
		"react-scripts": "3.0.1"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	}
}
